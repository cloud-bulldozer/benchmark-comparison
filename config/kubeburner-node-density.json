{
  "elasticsearch": {
    "ripsaw-kube-burner*": [
      {
        "filter": {
          "metricName.keyword": "jobSummary"
        },
        "buckets": [
          "metricName.keyword"
        ],
        "aggregations": {
          "elapsedTime": [ "max" ]
        }
      },
      {
        "filter": {
          "metricName.keyword": "podLatencyMeasurement"
        },
        "buckets": [
          "metricName.keyword"
        ],
        "aggregations": {
          "podReadyLatency": [
            {
              "percentiles": {
                "percents": [
                  0,1,5,25,50,75,95,99,100
                ]
              }
            }
          ],
          "initializedLatency": [
            {
              "percentiles": {
                "percents": [
                  0,1,5,25,50,75,95,99,100
                ]
              }
            }
          ],
          "schedulingLatency": [
            {
              "percentiles": {
                "percents": [
                  0,1,5,25,50,75,95,99,100
                ]
              }
            }
          ]
        }
      },
      {
        "filter": {
          "metricName.keyword": "containerCPU"
        },
        "buckets": [
          "labels.container.keyword"
        ],
        "aggregations": {
          "value": [
            {
              "percentiles": {
                "percents": [
                  0,1,5,25,50,75,95,99,100
                ]
              }
            }
          ]
        }
      },
      {
        "filter": {
          "metricName.keyword": "containerMemory"
        },
        "buckets": [
          "labels.container.keyword"
        ],
        "aggregations": {
          "value": [
            {
              "percentiles": {
                "percents": [
                  0,1,5,25,50,75,95,99,100
                ]
              }
            }
          ]
        }
      },
      {
        "filter": {
          "metricName.keyword": "kubeletCPU"
        },
        "buckets": [
          "metricName.keyword"
        ],
        "aggregations": {
          "value": [
            {
              "percentiles": {
                "percents": [
                  0,1,5,25,50,75,95,99,100
                ]
              }
            }
          ]
        }
      },
      {
        "filter": {
          "metricName.keyword": "kubeletMemory"
        },
        "buckets": [
          "metricName.keyword"
        ],
