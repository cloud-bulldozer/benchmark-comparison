{
  "elasticsearch": {
    "metadata": {
      "cpuinfo-metadata": {
        "additional_fields": [
          "pod_name"
        ],
        "fields": [
          "value.Model name",
          "value.Architecture",
          "value.CPU(s)"
        ]
      },
      "meminfo-metadata": {
        "additional_fields": [
          "pod_name"
        ],
        "fields": [
          "value.MemTotal"
        ]
      }
    },
    "ripsaw-ycsb-summary": [
      {
        "filter": {
          "phase": "run",
          "workload_type": "workloada"
        },
        "buckets": [
          "iteration",
          "recordcount",
          "operationcount",
          "driver.keyword"
        ],
        "aggregations": {
          "data.OVERALL.Throughput(ops/sec)": [
            "max"
          ],
          "data.UPDATE.95thPercentileLatency(us)": [
            "max"
          ],
          "data.READ.95thPercentileLatency(us)": [
            "max"
          ]
        }
      },
